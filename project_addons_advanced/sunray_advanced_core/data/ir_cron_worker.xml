<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <!-- Sunray IMQ Worker -->
        <record id="ir_cron_worker_sunray" model="ir.cron">
            <field name="name">IMQ Worker - sunray</field>
            <field name="model_id" ref="inouk_message_queue.model_imq_worker"/>
            <field name="state">code</field>
            <field name="code">model.process_message_queue('sunray', 'worker_sunray')</field>
            <field name="interval_number" eval="1"/>
            <field name="interval_type">minutes</field>
            <field name="imq_is_worker" eval="True"/>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="priority" eval="5"/>
        </record>
    </data>
</odoo>